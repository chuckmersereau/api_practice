$ ->
  $(document).on 'click', '[data-behavior="send_chalkline_list"]', ->
    if window.confirm('Would you like MPDX to email Chalkline your newsletter list and open their order form in a new tab?')
      window.open('http://www.chalkline.org/order_mpdx.html','_blank')
      $.ajax({type: 'post', url: '/contacts/send_to_chalkline'})
